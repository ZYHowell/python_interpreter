def test2_f(a):
    if a < 0:
        return 0
    if a % 4 == 1:
        return test2_g(a - 1) + 1
    elif a % 4 == 2:
        return test2_f(a - 1) + 1
    else:
        return test2_g(a) + 1


def test2_g(b):
    if b < 0:
        return 0
    if b % 4 == 3:
        return test2_f(b - 1) + 1
    elif b % 4 == 0:
        return test2_g(b - 1) + 1
    else:
        return test2_f(b) + 1


print(test2_f(100), test2_g(100))

